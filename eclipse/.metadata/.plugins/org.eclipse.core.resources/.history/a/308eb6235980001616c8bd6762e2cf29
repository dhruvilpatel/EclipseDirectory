
public class MaxSubArray {

	
	
	public static void main (String args[]){
		
	int[] arr =  {1,2}; 
		System.out.println(check_Maxsubarray(arr));
		System.out.println("check_Maxsubarray(arr)");
	}
	
	static int check_Maxsubarray(int[] arr){
		int ans = 0;
		for (int i = 0 ; i <arr.length ; ++i){
			int sum = 0;
			for (int j = 0 ; j < i ; ++j ){
				
				if( i + j > arr.length)
					break;
				sum += arr[i+j];
				ans  = Math.max(ans,sum);
			}
		}
		return ans;	

		
	}
	
}
