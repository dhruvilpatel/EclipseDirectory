
public class KrackenCount {

	public static void main (String args[]){
		int n = 2, m= 3;
		int right = n-1;
		int down = m-1;
		
		int ans = factorial(right+down)/(factorial(right)*factorial(down));
		System.out.println(ans);
		
		
	}
	static int factorial(int n){
		if(n==0){
			return 1;
		}
		return n * factorial(n-1);
	}
}
