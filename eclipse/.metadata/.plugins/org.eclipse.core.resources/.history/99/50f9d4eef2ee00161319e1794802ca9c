package zappos;

public class solution {

	public static void main (String args[]){
		int sum = 20;
		int[]arr = {1,2,3,4,5,6}; 
		 for (int i=0; i<arr.length; ++i)
	        {
	            int temp = sum-arr[i];
	 
	            // checking for condition
	            if (temp>=0 && binmap[temp])
	            {
	                System.out.println("Pair with given sum " +
	                                    sum + " is (" + arr[i] +
	                                    ", "+temp+")");
	            }
	            binmap[arr[i]] = true;
	        }
	}
	
}
