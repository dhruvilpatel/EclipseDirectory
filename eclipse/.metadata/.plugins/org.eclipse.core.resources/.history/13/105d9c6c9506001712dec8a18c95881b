/**
 * This is the Descriptor class which is Task 1 1st bullet point
 * author : acheruvu
 */

package global;

public class Descriptor {
    // Start: Changes Made By Us
    public double[] value;

    public Descriptor() {
        value = new double[5];
        value[0] = 0;
        value[1] = 0;
        value[2] = 0;
        value[3] = 0;
        value[4] = 0;
    }

    void set(double val0, double val1, double val2, double val3, double val4) {
        value[0] = val0;
        value[1] = val1;
        value[2] = val2;
        value[3] = val3;
        value[4] = val4;

    }

    double get(int index) {
        return value[index];
    }

    double equal(Descriptor obj) {
        int i = 0;
        for (i = 0; i < 5; i++) {
            if (this.value[i] != obj.value[i])
                return 0;
        }
        if (i == 5)
            return 1;
        else
            return 0;
    }

    public double distance(Descriptor obj) {
        double val = 0;
        ;
        for (int i = 0; i < 5; i++) {
            val = val + ((this.value[i] - obj.value[i]) * (this.value[i] - obj.value[i]));
        }

        val = Math.sqrt(val);
        return val;

    }

}