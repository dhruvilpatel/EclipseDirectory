
public class KrackenCount {

	public static void main (String args[]){
		int[][] a = { {1,2,3},{4,5,6},{7,8,9}};
		int n = 0 , m= 0;
		int[][] result = new int[n][m];
		for (int i = 0 ; i < n ; i++){
			result[i][0] = 1;
		}
		for (int i = 0 ; i < m ; i++){
			result[0][i] = 1;
		}
		for ( int i = 1 ; i < n ; i++){
			for ( int j = 1 ; j < m ; j++){
				result[i][j] = result[i][j-1] + result[i-1][j] + result[i-1][j-1];
			}

		}
		System.out.println(result[n-1][m-1]);
	}
	
}
