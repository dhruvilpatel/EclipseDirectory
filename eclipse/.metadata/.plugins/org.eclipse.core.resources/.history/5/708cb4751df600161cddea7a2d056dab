package HackerRank;

import java.util.Scanner;

public class GameOfTwoStack {
	public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int g = in.nextInt();
        for(int a0 = 0; a0 < g; a0++){
        	int[] a = {4,2,4,6,1};
        	int[] b = {2,1,8,5};
            int n = a.length;
            int m = b.length;
            int x = in.nextInt();
            int[] a = new int[n];
            int aCount = 0;
            int bCount = 0;
            int check = x;
            for(int a_i=0; a_i < n; a_i++){
                a[a_i] = in.nextInt();
                if(a[a_i] <= check){
                    aCount++;
                    check -= a[a_i];
                }else{
                    check = 0;
                }
            }
            int[] b = new int[m];
            check = x;
            for(int b_i=0; b_i < m; b_i++){
                b[b_i] = in.nextInt();
                if(b[b_i] < check){
                    bCount++;
                    check -= b[b_i];
                }else{
                    check = 0;
                }
            }
            // your code goes here
            int mixCount = 0;
            check = x;
            int i = 0,j = 0;
            while(check >= 0 && (check > a[i] || check > b[j])){
                if(a[i]>b[j]){
                        check -= b[j];
                        j++;
                    }
                else{
                    check -= a[i];
                        i++;
                }
                mixCount++;
                
            }
            System.out.println(Math.max(Math.max(aCount,bCount),mixCount));
        }
        
    }
}
